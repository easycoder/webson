{
    "#": ["$Import", "$Parse", "$NItems", "$Structure"],
    
    "$Import": {
        "#element": "textarea",
        "@id": "ImportPanel",
        "display": "none",
        "width": "100%",
        "height": "20em"
    },
    
    "$Parse": {
        "#element": "button",
        "@id": "Parse",
        "@type": "button",
        "#content": "Parse",
        "display": "none",
        "width": "100%"
    },
    
    "$NItems": {
        "#element": "div",
        "@id": "NItems",
        "#content": 5,
        "display": "none"
    },
    
    "$Structure": {
        "#repeat": {
            "#doc": "This repeats an item vertically",
            "#target": "$Item",
            "#steps": {
                "#contentof": "$NItems"
            }
        },
        
        "$Item": {
            "#element": "div",
            "#content": "#step"
        }
    }
}
